{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "no-console": 1,
    "vue/no-multiple-template-root": 0,
    "consistent-return": 0
  }
}
